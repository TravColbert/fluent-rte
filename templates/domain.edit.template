			</tr>
			<tr>
				<td>Proxy 1:</td><td><input type="text" id="proxy1" name="proxy1" value="{domain/proxy1}"/><input type="hidden" id="orig_proxy1" name="orig_proxy1" value="{domain/proxy1}"/></td>
			</tr>
			<tr>
				<td>Proxy 1 Port:</td><td><input type="text" id="proxy1port" name="proxy1port" value="{domain/proxy1port}"/><input type="hidden" id="orig_proxy1port" name="orig_proxy1port" value="{domain/proxy1port}"/></td>
			</tr>
			<tr>
