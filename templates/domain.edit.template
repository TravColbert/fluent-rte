	<form name="create" action="index.php" method="post">
	<input type="hidden" id="resource" name="resource" value="domain"/>
	<input type="hidden" id="template" name="template" value="domain.table.template"/>
	<input type="hidden" id="action" name="action" value="edit"/>
	<input type="hidden" id="id" name="id" value="{domain/id}"/>
	<table>
		<thead>
			<tr>
				<th>Action:</th><th>Edit Domain</th>
			</tr>
		</thead>
		<tbody>
			<tr>
