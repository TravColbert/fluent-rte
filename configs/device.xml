<?xml version="1.0"?>
<registry>
    <device>
    	<datasource verb="GET">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
    	<datasource verb="POST">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
		<keyfield>id</keyfield>
		<fields>id,devicetype_id:int,domain_id:int,description</fields>
		<successresult>device</successresult>
    </device>
</registry>
