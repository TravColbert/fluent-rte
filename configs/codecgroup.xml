<?xml version="1.0"?>
<registry>
    <codecgroup>
	 <datasource verb="GET">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
	<datasource verb="POST">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
		<keyfield>id</keyfield>
		<fields>id:int,domain_id:int,codec_id,priority:int</fields>
		<successresult>codecgroup</successresult>
    </codecgroup>
</registry>
