<?xml version="1.0"?>
<registry>
	<subscriber>
		<datasource verb="GET">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
		<datasource verb="POST">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
		<keyfield>id</keyfield>
		<fields>id:int,username,password,domain,screenname,rpid,email_address,vmail,shared:bool,enabled:bool</fields>
		<successresult>subscriber</successresult>
	</subscriber>
</registry>
