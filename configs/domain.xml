<?xml version="1.0"?>
<registry>
    <domain>
    	<datasource verb="GET">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
    	<datasource verb="POST">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
		<keyfield>id</keyfield>
		<fields>id:int,domain,reg1,reg1port,reg2,reg2port,proxy1,proxy1port,proxy2,proxy2port,outproxy,outproxyport,mohurl,time,regtimeout,dialplan,description</fields>
		<successresult>domain</successresult>
    </domain>
</registry>
