<?xml version="1.0"?>
<registry>
    <devicetype>
    	<datasource verb="GET">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
    	<datasource verb="POST">
			<object>table</object>
			<url>table.class.php</url>
			<output>xml</output>
		</datasource>
		<keyfield>id</keyfield>
		<fields>id:int,make,model,template,description</fields>
		<successresult>devicetype</successresult>
    </devicetype>
</registry>
